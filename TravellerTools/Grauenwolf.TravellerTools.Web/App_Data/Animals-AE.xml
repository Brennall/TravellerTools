            <Option Roll="4" Behavior="Hunter" Reaction="0" />
            <Option Roll="5" Behavior="Intermittent" Reaction="-1" />
            <Option Roll="6" Behavior="Reducer" Reaction="-2" />
          </Behaviors>
        </Option>
      </Diets>
      <Skills>
        <Skill Name="Athletics" Score="0" />
        <Skill Name="Recon" Score="0" />
        <Skill Name="Survival" Score="0" />
