      <Chart Name="PhysicalSkills">
        <Option Roll="1">
          <Attribute Name="Dex" Bonus="1"/>
        </Option>
        <Option Roll="2" >
          <Attribute Name="Str" Bonus="2"/>
        </Option>
        <Option Roll="3">
          <Attribute Name="End" Bonus="1"/>
        </Option>
        <Option Roll="4">
          <Attribute Name="End" Bonus="2"/>
        </Option>
        <Option Roll="5">
          <Attribute Name="Dex" Bonus="1D6"/>
        </Option>
        <Option Roll="6">
          <Skill Name="Melee" Bonus="1"/>
        </Option>
      </Chart>

      <Chart Name="SocialSkills">
        <Option Roll="1">
          <Attribute Name="Pack" Bonus="4"/>
        </Option>
        <Option Roll="2" >
          <Attribute Name="Instinct" Bonus="1"/>
        </Option>
        <Option Roll="3">
          <Skill Name="Deception" Bonus="1"/>
        </Option>
        <Option Roll="4">
          <Attribute Name="Instinct" Bonus="1"/>
        </Option>
        <Option Roll="5">
          <Skill Name="Deception" Bonus="1"/>
        </Option>
        <Option Roll="6">
          <Skill Name="Recon" Bonus="1"/>
        </Option>
      </Chart>

      <Chart Name="EvolutionSkills">
        <Option Roll="1">
          <Feature Text="+1D6 Damage"/>
        </Option>
        <Option Roll="2" >
          <Attribute Name="Armor" Bonus="1" />
        </Option>
        <Option Roll="3">
          <Attribute Name="Int" Bonus="1" />
        </Option>
        <Option Roll="4">
          <Attribute Name="PhysicalSkills" Bonus="1" />
        </Option>
        <Option Roll="5">
          <Attribute Name="SocialSkills" Bonus="1" />
        </Option>
        <Option Roll="6">
          <Feature Text="Exotic Weapon"/>
        </Option>
      </Chart>

      <Chart Name="Quirks">
        <Option Roll="2">
          <Feature Text="Unusual Vocalization"/>
        </Option>
        <Option Roll="3">
          <Feature Text="Blind" />
          <Skill Name="Recon" Bonus="-1"/>
        </Option>
        <Option Roll="4">
          <Feature Text="Silent"/>
          <Skill Name="Stealth" Score="0"/>
        </Option>
        <Option Roll="5">
          <Feature Text="Bright Colorization"/>
        </Option>
        <Option Roll="6">
          <Feature Text="Found everywhere"/>
          <Skill Name="Survival" Bonus="2" />
        </Option>
        <Option Roll="7">
          <Feature Text="Attractive Scent (Siren)" />
          <Attribute Name="Pack" Bonus="-4"/>
          <Skill Name="Deception" Bonus="1"/>
          <Chart Roll="1D2">
            <Option Roll="1">
              <!-- No effect-->
            </Option>
            <Option Roll="2">
              <Feature Text="Cannot Move" />
              <Attribute Name="End" Bonus="2"/>
              <Skill Name="Survival" Bonus="2" />
            </Option>
          </Chart>
        </Option>
        <Option Roll="8">
          <Feature Text="Loud scream when threatened"/>
        </Option>
        <Option Roll="9">
          <Feature Text="Swarming"/>
          <Feature Text="Roll 2D6: On 2, becomes Killer for duration of encounter." />
        </Option>
        <Option Roll="10">
          <Feature Text="Stench Exotic Weapon" />
        </Option>
        <Option Roll="11">
          <Feature Text="Ridged Shell"/>
          <Attribute Name="Armor" Bonus="2" />
        </Option>
        <Option Roll="12">
          <Attribute Name="Quirks" Bonus="2"/>
        </Option>
      </Chart>

    </AnimalClass>

