          <Attribute Name="Str" Bonus="1"/>
          <Attribute Name="EvolutionDM" Bonus="1"/>
          <Skill Name="Melee" Score="0" />
          <Behaviors>
            <Option Roll="1" Behavior="Pouncer" Reaction="-1" />
            <Option Roll="2" Behavior="Trapper" Reaction="-2" />
            <Option Roll="3" Behavior="Hunter" Reaction="-1"/>
            <Option Roll="4" Behavior="Hunter" Reaction="-1" />
            <Option Roll="5" Behavior="Hunter" Reaction="0" />
            <Option Roll="6" Behavior="Chaser" Reaction="-2" />
          </Behaviors>
        </Option>
        <Option Odds="1" Diet="Herbivore" >
